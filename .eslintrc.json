{
  "extends": ["next", "next/core-web-vitals", "prettier", "plugin:testing-library/react"],
  "plugins": ["prettier", "testing-library"],
  "rules": {
    "prettier/prettier": "error",
    "@next/next/no-img-element": "off"
  },
  "overrides": [
    {
      "files": ["**/?(*.)+test.ts?(x)"],
      "extends": ["plugin:testing-library/react"]
    }
  ]
}
